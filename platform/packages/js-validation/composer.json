{
    "name": "botble/js-validation",
    "type": "library",
    "description": "Validate forms transparently with Javascript reusing your Laravel Validation Rules, Messages, and FormRequest",
    "license": "MIT",
    "require": {
    },
    "autoload": {
        "psr-4": {
            "Botble\\JsValidation\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Botble\\JsValidation\\Providers\\JsValidationServiceProvider"
            ],
            "aliases": {
                "JsValidator": "Botble\\JsValidation\\Facades\\JsValidatorFacade"
            }
        }
    }
}
